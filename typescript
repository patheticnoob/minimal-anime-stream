export async function fetchYumaRecentEpisodes(page: number = 1)
{
  let animeId = item.id;
  if (typeof animeId === 'string' && animeId.includes('-episode-')) {
    animeId = animeId.replace(/-episode-\d+$/, '');
  }
}

